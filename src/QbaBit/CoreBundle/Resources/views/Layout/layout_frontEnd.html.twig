                    <p style="color: #fff;"><i class="fa fa-bank"> </i> {{ central.nombre }},</p>
                    <p style="color: #fff;"><i class="fa fa-map-marker"></i> {{ central.direccion }}</p>
                    <p style="color: #fff;"><i class="fa fa-phone"></i> {{ central.idUsuario.telFijo }}</p>
                    <a title="" href="mailto:{{ central.idUsuario.email }}"><i
                                class="fa fa-envelope"></i> {{ central.idUsuario.email }}</a>
                </address>
            </aside>

            {#<aside class="col-md-3 col-sm-3 widget widget-link">#}
