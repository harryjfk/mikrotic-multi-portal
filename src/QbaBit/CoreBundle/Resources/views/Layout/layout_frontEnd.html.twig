            {#<li><a title="Latest News" href="#"><i class="fa fa-tint"></i> Latest News</a></li>#}
            {#</ul>#}
            {#</aside>#}

            <aside class="col-md-4 col-sm-4 widget widget-link  slider-section" style="padding-right: 0px">
                {{ render(controller('FrontBundle:Default:contacto')) }}
            </aside>

            <!-- Footer Bottom -->
            <div class="footer-bottom">
                <p>{{ 'copyright' | trans({"%anyo%": 'now' | date('Y') }, 'frontend') }}</p>
            </div>
