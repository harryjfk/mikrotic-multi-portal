    <script type="text/javascript">
        jQuery(function () {
            jQuery('.contact-form').on('submit', function (e) {
                e.preventDefault();
                MKLib.AjaxRequest('{{ path('front_contacto') }}', 'quick-contact', function (j) {
                    if (j.status === 1) {
                        MKLib.showMsg('alert-success', '{{ 'js_contacto_enviado' | trans({}, 'messages') }}');
                        MKLib.resetForm('quick-contact');
                        Util.removeMsgIfNotErrors();
                        MKLib.unlockScreen();
                    }
                    else
                        MKLib.showMsg('alert-danger', '{{ 'js_contacto_no_enviado' | trans({}, 'messages') }}', '#quick-contact');
                    Util.renderErrors(j.error);
                    MKLib.unlockScreen();
                }, 'json', 'post');
            });

        });
    </script>

