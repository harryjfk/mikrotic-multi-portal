{% block body %}
    {% set central = TwigServiceDooCentral.getCentral() %}

    <a id="top"></a>
    <header id="header-section" class="header-section">
        <!-- Top Header -->
        {{ render(controller('FrontBundle:RenderSections:top_header_user', { request: app.request })) }}
        <!-- Top Header /- -->

        <!-- Logo Block -->
        <div class="logo-block">
            <!-- container -->
            <div class="container">
                <div class="row">
                    <!-- col-md-2 -->
                    <div class="col-md-3 col-sm-4">
                        <a title="Logo" href="{{ path('front_homepage') }}">
                            <img alt="Lavadoo" title="Lavadoo"
                                 src="{{ asset('bundles/front/images/compose_logo.png') }}">
                        </a>
                    </div><!-- col-md-2 /- -->
                    <!-- col-md-4 -->
                    <div class="col-md-6 col-sm-8 pull-right row">
                        <!-- col-md-7 -->
                        <div class="col-md-6 col-sm-6 col-sm-offset-2 col-md-offset-2 call-us">
                            <img src="{{ asset('bundles/qbabittemplate/makeclean/images/icon/mobile-icon.png') }}"
                                 alt="mobile-icon"/>
                            <p>{{ 'contactenos' | trans({}, 'frontend') }} !<span>{{ central.idUsuario.telFijo }}</span>
                            </p>
                        </div><!-- col-md-7 /- -->
                        <!-- col-md-5 -->
                        <span id="smallcart">{{ render(controller('FrontBundle:DooCart:smallcart')) }}</span>
                        {#<div class="cart col-md-4 col-sm-4 text-right ow-padding-left">#}
                        {#<p>#}
                        {#<i class="cart-icon">2</i>#}
                        {#cart - $ 25.00#}
                        {#</p>#}

                        {#</div><!-- col-md-5 /- -->#}
                    </div><!-- col-md-4 /- -->
                </div>
            </div>
        </div><!-- Logo Block /- -->

        <!-- Menu Block -->
        <div class="menu-block">
            <div class="container">
                <div class="row">
                    <!-- col-md-8 -->
                    <nav class="navbar navbar-default col-md-12">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                                    data-target="#bs-example-navbar-collapse-1">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a title="Logo" href="{{ path('front_homepage') }}">
                                <img alt="Lavadoo" title="Lavadoo"
                                     src="{{ asset('bundles/front/images/compose_logo.png') }}">
                            </a>
                        </div>
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <a class="logo-nav-fixed" title="Logo" href="{{ path('front_homepage') }}">
                                <img alt="Lavadoo" title="Lavadoo" src="{{ asset('bundles/front/images/compose_logo.png') }}">
                            </a>
                            <ul class="nav navbar-nav">
                                {{ render(controller('QbabitMenuBundle:Default:menu_header_front', { request: app.request })) }}

                                {% if not is_granted('IS_AUTHENTICATED_FULLY') %}
                                    <li>
                                        <a href="{{ path('front_registrarse') }}">{{ 'registarse' | trans({}, 'frontend') }}</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('front_user_login') }}">{{ 'entrar_tu_cuenta' | trans({}, 'seguridad') }}</a>
                                    </li>
                                {% else %}
                                    <li>
                                        <a href="{{ path('front_user_logout') }}">{{ 'cerrar_sesion' | trans({}, 'seguridad') }}</a>
                                    </li>
                                {% endif %}
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </nav><!-- col-md-8 /- -->
                </div>
            </div><!-- /.container -->
        </div>

        <!-- Menu Block /- -->
    </header>
    <section class="content_up_page">
        {% block uppercontentpage %}{% endblock %}
    </section>
    <section id="industry-serve-section" class="industry-serve-section ow-section">
        {% block update %}{% endblock %}
    </section>

    <!-- Footer Section -->
    <footer id="footer-section" class="footer-section ow-background">
        <!-- container -->
        <div class="container">

            {#<div class="footer-heading">#}
            {#<h5>Have any questions or want a free estimate?</h5>#}
            {#<h3>Call Now: +(01) 800 527 4800</h3>#}
